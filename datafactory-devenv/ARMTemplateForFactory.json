						],
						"outputs": [
							{
								"referenceName": "target_azsqldb",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": [],
				"lastPublishTime": "2022-05-10T03:31:41Z"
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/ds_csv_file')]",
				"[concat(variables('factoryId'), '/datasets/target_azsqldb')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/target_azsql')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "azsqldatabase",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [],
