{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory-devenv"
		},
		"SqlServer_linkservice_onprem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer_linkservice_onprem'"
		},
		"blobcsvfile_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blobcsvfile'"
		},
		"azsqldatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azsqldatabase'"
		},
		"ds_blob_file_conn_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ds_blob_file_conn'"
		},
		"conn_azsqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'conn_azsqldb'"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
