{
	"name": "df_Customer",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "cust_csv1",
						"type": "DatasetReference"
					},
					"name": "c1"
				},
				{
					"dataset": {
						"referenceName": "DelimitedText5",
						"type": "DatasetReference"
					},
					"name": "c2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable6",
						"type": "DatasetReference"
					},
					"name": "customer"
				}
			],
			"transformations": [
				{
					"name": "custunion"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ID as short,",
				"          CompaniesName as string,",
				"          Address1 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> c1",
				"source(output(",
				"          ID as short,",
				"          CompaniesName as string,",
				"          Address as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> c2",
				"c1, c2 union(byName: false)~> custunion",
				"custunion sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          ID,",
				"          CompaniesName,",
				"          Address = Address1",
				"     )) ~> customer"
			]
		}
	}
}